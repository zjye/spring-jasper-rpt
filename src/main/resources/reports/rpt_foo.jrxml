<?xml version="1.0" encoding="UTF-8"?>
<jasperReport name="Simple_Report">
    <field name="country" class="java.lang.String"></field>
    <field name="name" class="java.lang.String"></field>
    <pageHeader></pageHeader>
    <columnHeader>
        <band height = "23">

            <staticText>
                <text><![CDATA[]]> </text>
            </staticText>

            <staticText>
                <text><![CDATA[Country]]></text>
            </staticText>

            <staticText>
                <text><![CDATA[Name]]></text>
            </staticText>

        </band>
    </columnHeader>
    <detail>
        <band height = "16">

            <staticText>
                <text><![CDATA[]]> </text>
            </staticText>

            <textField>
                <textFieldExpression class = "java.lang.String">
                    <![CDATA[$F{country}]]>
                </textFieldExpression>
            </textField>

            <textField>
                <textFieldExpression class = "java.lang.String">
                    <![CDATA[$F{name}]]>
                </textFieldExpression>
            </textField>

        </band>
    </detail>
    <columnfooter></columnfooter>
</jasperReport>
